{
    "name": "dasun4u/laravel-ideabiz-handler",
    "description": "Laravel package for IDEABIZ REST API handling",
    "license": "MIT",
    "authors": [
        {
            "name": "Dasun Dissanayake",
            "email": "dpdasun@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "7.4.2"
    },
    "autoload": {
